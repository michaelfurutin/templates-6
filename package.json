{
  "name": "@ottofeller/templates",
  "description": "Projen templates for OttoFeller projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/ottofeller/templates.git"
  },
  "scripts": {
    "build": "npx projen build",
    "clobber": "npx projen clobber",
    "compat": "npx projen compat",
    "compile": "npx projen compile",
    "default": "npx projen default",
    "eject": "npx projen eject",
    "format": "npx projen format",
    "lint": "npx projen lint",
    "package": "npx projen package",
    "package-all": "npx projen package-all",
    "package:js": "npx projen package:js",
    "post-compile": "npx projen post-compile",
    "pre-compile": "npx projen pre-compile",
    "test": "npx projen test",
    "test:watch": "npx projen test:watch",
    "typecheck": "npx projen typecheck",
    "watch": "npx projen watch",
    "projen": "npx projen"
  },
  "author": {
    "name": "ottofeller",
    "url": "https://ottofeller.com",
    "organization": false
  },
  "devDependencies": {
    "@ottofeller/eslint-config-ofmt": "1.7.2",
    "@ottofeller/ofmt": "1.7.2",
    "@ottofeller/prettier-config-ofmt": "1.7.2",
    "@types/jest": "^27",
    "@types/jscodeshift": "^0.11.6",
    "@types/node": "^16",
    "@types/prettier": "2.6.0",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint-plugin-import": "2.25.4",
    "jest": "^27",
    "jest-junit": "^15",
    "jsii": "1.x",
    "jsii-diff": "^1.61.0",
    "jsii-pacmak": "^1.61.0",
    "projen": "^0.70.15",
    "ts-jest": "^27",
    "ts-node": "10.8.1",
    "typescript": "4.7.4"
  },
  "peerDependencies": {
    "projen": "^0.70.15"
  },
  "dependencies": {
    "@ottofeller/prettier-config-ofmt": "1.5.2",
    "prettier": "2.8.0",
    "projen": "^0.70.15"
  },
  "bundledDependencies": [
    "@ottofeller/prettier-config-ofmt",
    "prettier"
  ],
  "engines": {
    "node": ">= 16.0.0"
  },
  "main": "lib/index.js",
  "license": "Apache-2.0",
  "version": "1.4.3",
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/(test|src)/**/*(*.)@(spec|test).ts?(x)"
    ],
    "clearMocks": true,
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "clover",
      "cobertura",
      "text"
    ],
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "/node_modules/"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test-reports"
        }
      ]
    ],
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.dev.json"
      }
    }
  },
  "types": "lib/index.d.ts",
  "stability": "stable",
  "jsii": {
    "outdir": "dist",
    "targets": {},
    "tsc": {
      "outDir": "lib",
      "rootDir": "src"
    }
  },
  "prettier": "@ottofeller/prettier-config-ofmt",
  "eslintConfig": {
    "extends": [
      "@ottofeller/eslint-config-ofmt/eslint.quality.cjs",
      "@ottofeller/eslint-config-ofmt/eslint.formatting.cjs"
    ]
  },
  "overrides": {
    "@types/babel__traverse": "7.18.2",
    "@types/prettier": "2.6.0"
  },
  "//": "~~ Generated by projen. To modify, edit .projenrc.ts and run \"npx projen\"."
}
